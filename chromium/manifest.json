{
  "manifest_version": 3,
  "name": "Aenzbi Suite",
  "description": "Suite of apps for POS, Accounting, CRM, Games, Media, and Dashboard.",
  "version": "1.0",
  "permissions": ["storage"],
  "host_permissions": ["https://*/", "http://*/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
